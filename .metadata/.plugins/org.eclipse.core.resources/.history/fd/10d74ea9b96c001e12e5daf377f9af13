public class CollectionDemo {

	public static void main(String[] args) {
		List lst = new ArrayList();
		lst.add(100);
		lst.add(200);
		lst.add("abc");
		lst.add(10.02f);
		lst.add(new Employee());
		
		// Read
		//for, foreach, Iterator, ListIterator
		for(int i=0; i<lst.size(); i++) {
			System.out.println(lst.get(i));
		}
		
		
		         
		
		
		
